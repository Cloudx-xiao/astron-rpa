{
  "name": "@rpa/tokens",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/variables.js",
  "module": "./dist/variables.js",
  "types": "./dist/variables.d.ts",
  "scripts": {
    "build": "style-dictionary build --config config.js",
    "postbuild": "node scripts/merge-css.js",
    "postinstall": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/variables.d.ts",
      "import": "./dist/variables.js",
      "require": "./dist/variables.cjs"
    },
    "./dark": {
      "types": "./dist/variables.d.ts",
      "import": "./dist/variables.dark.js",
      "require": "./dist/variables.dark.cjs"
    },
    "./css-variables": {
      "types": "./dist/variables.d.ts",
      "import": "./dist/variables.css.js",
      "require": "./dist/variables.css.js"
    },
    "./variables.css": "./dist/variables.css",
    "./variables.dark.css": "./dist/variables.dark.css",
    "./variables.scss": "./dist/variables.scss",
    "./tailwind": "./dist/tailwind-preset.js"
  },
  "devDependencies": {
    "ant-design-vue": "^4.2.3",
    "lodash-es": "^4.17.21",
    "style-dictionary": "^4.3.3"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0"
  }
}
